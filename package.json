{
  "name": "wolf-builder",
  "version": "0.0.5",
  "description": "wolf-builder",
  "main": "index.js",
  "author": "deng00001",
  "scripts": {
    "clean": "rm -rf lib && rm -rf es6 && rm -rf typings",
    "build:ts": "tsc -p ./ --outDir es6/",
    "build:cjs": "cross-env BABEL_ENV=cjs babel es6 --out-dir lib",
    "build": "npm run clean && npm run build:ts && npm run build:cjs",
    "prepublish": "npm run clean && npm run build",
    "webpack": "./bin/wolf-builder",
    "web:build": "cross-env NODE_ENV=production webpack --config './lib/dev.js' ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "wolf-builder": "./bin/wolf-builder"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6"
          }
        }
      ]
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "dependencies": {
    "autoprefixer": "^9.4.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^1.0.0",
    "commander": "^2.19.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "image-webpack-loader": "^4.6.0",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.3",
    "svgo-loader": "^2.2.0",
    "ts-loader": "^5.3.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "devDependencies": {
    "@types/node": "^10.12.12",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "typescript": "^3.2.2"
  }
}
